{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-plusplus": "off",
    "eslint-plugin-import/prefer-default-export": "off",
    "no-restricted-globals": "off",
    "eslintimport/prefer-default-export": "off",
    "no-use-before-define": "off",
    "eqeqeq": "off",
    "no-param-reassign": "off",
    "eslint-plugin-import/no-extraneous-dependencies": "off"
  }
}
